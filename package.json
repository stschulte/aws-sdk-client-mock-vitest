{
  "name": "aws-sdk-client-mock-vitest",
  "version": "7.0.0",
  "description": "Custom matchers for AWS SDK Client mock to be used in vitest",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || >=24.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./extend": {
      "types": "./dist/extend.d.ts",
      "default": "./dist/extend.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "validate": "tsc --noEmit",
    "all": "npm run validate && npm run lint && npm run build && npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stschulte/aws-sdk-client-mock-vitest.git"
  },
  "author": "Stefan Schulte <stschulte@posteo.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stschulte/aws-sdk-client-mock-vitest/issues"
  },
  "homepage": "https://github.com/stschulte/aws-sdk-client-mock-vitest#readme",
  "peerDependencies": {
    "@smithy/types": ">=3.5.0",
    "aws-sdk-client-mock": ">=2.2.0",
    "vitest": ">=3.2.0"
  },
  "dependencies": {
    "@vitest/expect": "^4.0.7"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.925.0",
    "@aws-sdk/client-secrets-manager": "^3.925.0",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/eslint-plugin": "^1.4.1",
    "@vitest/utils": "^4.0.7",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^9.39.1",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.7"
  }
}
